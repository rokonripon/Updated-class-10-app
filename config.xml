<?xml version='1.0' encoding='utf-8'?>
<widget id="com.writer.ripon" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Class 9-10 Science Helper</name>
    <description>নবম - দশম বিজ্ঞান সহায়িকা</description>
    <author email="rokonripon1@gmail.com">Rokonuzzaman Ripon</author>

    <content src="index.html" />

    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />

    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
</widget>
